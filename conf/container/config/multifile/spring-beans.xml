<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <!-- 
  
  Multiple File Import example
  
    - You can easily break up your config files and import them into
      a main file.
      
  -->

  <import resource="file-one.xml"/>
  <import resource="file-two.xml"/>
  <import resource="file-three.xml"/>

  <bean id="mainFile" 
        class="com.infinitemule.spring.container.config.multifile.MainFile">
  </bean>


</beans>
 